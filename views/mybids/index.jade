extends ../../layouts/default

block head
 title My Bids

block body
 div.row
  div.col-sm-8
   div.page-header
    h1 My Bids
   ul.events
    each event, i in data
     li
      strong #{event.anwsers} &nbsp;
      strong #{event.total} &nbsp;
      &nbsp;&nbsp; 
      &nbsp;&nbsp;
      i.fa.fa-trash 
      a(id='deleteBid', data-delete='#{event._id}', href='/bids/delete/#{event._id}')  Withdraw