extends ../../layouts/default

block head
 title My Bids

block body
 div.row
  div.col-sm-8
   div.page-header
    h1 My Bids
   ul.events
    each event, i in data
     li
      strong #{event.anwsers} &nbsp;
      strong #{event.jobID}   &nbsp;
      strong #{event.username}&nbsp;
      &nbsp;&nbsp; 
      i.fa.fa-pencil 
      a(href='/bids/edit/#{event._id}')  Edit
      &nbsp;&nbsp;
      i.fa.fa-trash 
      a(id='deleteEvent', data-delete='#{event._id}', href='#')  Delete